window.onload = MS_show_Wind_fields;



function MS_validate_for_Wind_common()
{ 
	var MS_sep = "#!#";
	
	// AMBITO TT per Wind 
	var MS_AmbitoTT_PM_Wind = '$Env{"MS_AmbitoTT_PM_Wind"}';
	var MS_AmbitoTT_PM_Wind_ARRAY = MS_AmbitoTT_PM_Wind.split(MS_sep);
	MS_AmbitoTT_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
	// Tipo Linea per Wind 
	var MS_TipoLinea_PM_Wind = '$Env{"MS_TipoLinea_PM_Wind"}';
	var MS_TipoLinea_PM_Wind_ARRAY = MS_TipoLinea_PM_Wind.split(MS_sep);
	MS_TipoLinea_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
	// MNP Type per Wind 
	var MS_MNPType_PM_Wind = '$Env{"MS_MNPType_PM_Wind"}';
	var MS_MNPType_PM_Wind_ARRAY = MS_MNPType_PM_Wind.split(MS_sep);
	MS_MNPType_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
	// Marcaggio per Wind 
	var MS_Marcaggio_PM_Wind = '$Env{"MS_Marcaggio_PM_Wind"}';
	var MS_Marcaggio_PM_Wind_ARRAY = MS_Marcaggio_PM_Wind.split(MS_sep);
	MS_Marcaggio_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
	
 	//valori correnti del ticket
 	var MS_current_ticket_AmbitoTT = '$Env{"MS_current_ticket_WIND_AMBITOTT"}';
 	var MS_current_ticket_TIPO_LINEA  = '$Env{"MS_current_ticket_WIND_TIPO_LINEA"}';
 	var MS_current_ticket_MNP_TYPE  = '$Env{"MS_current_ticket_WIND_MNP_TYPE"}';
 	var MS_current_ticket_freetext1  = '$QData{"TicketFreeText1"}';
 	





	var test_ok = false;
	
	for (var i=0;i<MS_AmbitoTT_PM_Wind_ARRAY.length;i++)
	{
		if(MS_current_ticket_AmbitoTT == MS_AmbitoTT_PM_Wind_ARRAY[i])
		{
			test_ok = true;
			break;
		}
	}

	if(!test_ok)
	{   					
		alert('$JSText{"AmbitoTT non corretto"}');
		return false;					
	}	
	
	
	
	

	if(MS_current_ticket_TIPO_LINEA != '')
	{
		test_ok = false;
		for (var i=0;i<MS_TipoLinea_PM_Wind_ARRAY.length;i++)
		{
			if(MS_current_ticket_TIPO_LINEA == MS_TipoLinea_PM_Wind_ARRAY[i])
			{
				test_ok = true;
				break;
			}
		}

		if(!test_ok)
		{   					
			alert('$JSText{"Tipo Linea non corretto"}');
			return false;					
		}
	}
	
	
	
	
	if(MS_current_ticket_MNP_TYPE != '')
	{
		test_ok = false;
		for (var i=0;i<MS_MNPType_PM_Wind_ARRAY.length;i++)
		{
			if(MS_current_ticket_MNP_TYPE == MS_MNPType_PM_Wind_ARRAY[i])
			{
				test_ok = true;
				break;
			}
		}

		if(!test_ok)
		{   					
			alert('$JSText{"MNP_TYPE non corretto"}');
			return false;					
		}
	}
	
	
	
	if(MS_current_ticket_freetext1 != '')
	{
		test_ok = false;
		for (var i=0;i<MS_Marcaggio_PM_Wind_ARRAY.length;i++)
		{
			if(MS_current_ticket_freetext1.toUpperCase() == MS_Marcaggio_PM_Wind_ARRAY[i].toUpperCase())
			{
				test_ok = true;
				break;
			}
		}

		if(!test_ok)
		{   					
			alert('$JSText{"Marcaggio non corretto"}');
			return false;					
		}
	}
			
 	return true;
}


function MS_validate_for_Wind()
{ 
	var queueDest = document.getElementsByName("DestQueueID")[0].value;
	
	
	var MS_sep = "#!#";
	
	// Categorie ALARM per Wind 
	var MS_Category_Alarm_PM_Wind = '$Env{"MS_Category_Alarm_PM_Wind"}';
	var MS_Category_Alarm_PM_Wind_ARRAY = MS_Category_Alarm_PM_Wind.split(MS_sep);
	MS_Category_Alarm_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
	// Categorie Incident per Wind 
	var MS_Category_Incident_PM_Wind = '$Env{"MS_Category_Incident_PM_Wind"}';
	var MS_Category_Incident_PM_Wind_ARRAY = MS_Category_Incident_PM_Wind.split(MS_sep);
	MS_Category_Incident_PM_Wind_ARRAY.sort(); //ordino alfabeticamente
	
   
   
   // i nuovi tipi di ticket
   var MS_ticketTypeID_IncidentToWind = '$Env{"MS_ticketTypeID_IncidentToWind"}';
   var MS_ticketTypeID_IncidentToPM = '$Env{"MS_ticketTypeID_IncidentToPM"}';
   var MS_ticketTypeID_IncidentToEricsson = '$Env{"MS_ticketTypeID_IncidentToEricsson"}';
   var MS_ticketTypeID_AlarmFromWind = '$Env{"MS_ticketTypeID_AlarmFromWind"}';
   var MS_ticketTypeID_AlarmToWind = '$Env{"MS_ticketTypeID_AlarmToWind"}';
   var MS_ticketTypeID_AlarmToEricsson = '$Env{"MS_ticketTypeID_AlarmToEricsson"}';
   var MS_ticketTypeID_AlarmToPM = '$Env{"MS_ticketTypeID_AlarmToPM"}';
   	
   	// nuove code
 	var MS_queue_id_incident_for_wind = '$Env{"MS_queue_id_incident_for_wind"}';  
 	var MS_queue_id_alarm_to_wind = '$Env{"MS_queue_id_alarm_to_wind"}';  
 	var MS_queue_id_alarm_from_wind = '$Env{"MS_queue_id_alarm_from_wind"}';  
 	var MS_queue_id_ericsson = '$Env{"MS_queue_id_ericsson"}';  
 	var MS_queue_id_MVNE_FE_Wind_Ericsson_Alarm = '$Env{"MS_queue_id_MVNE-FE-Wind-Ericsson-Alarm"}';  
 	var MS_queue_id_ERICSSON_PM_WIND_OUT = '$Env{"MS_queue_id_ERICSSON_PM_WIND_OUT"}';  
 	var MS_queue_id_MVNE_FE_Ericsson_Incident = '$Env{"MS_queue_id_MVNE-FE-Ericsson-Incident"}';  
 	var MS_queue_id_ERICSSON_WIND_OUT_Alarm = '$Env{"MS_queue_id_ERICSSON-WIND-OUT-Alarm"}';  
 	var MS_queue_id_WIND_ERICSSON_OUT_Alarm = '$Env{"MS_queue_id_WIND-ERICSSON-OUT-Alarm"}';  
 	var MS_queue_id_ERICSSON_OUT_Alarm = '$Env{"MS_queue_id_ERICSSON-OUT-Alarm"}';  
 	var MS_queue_id_MVNE_FE_Ericsson_Alarm = '$Env{"MS_queue_id_MVNE-FE-Ericsson-Alarm"}';  
 	var MS_queue_id_PM_Create = '$Env{"MS_queue_id_PM-Create"}';  
 	var MS_queue_id_ERICSSON_Create = '$Env{"MS_queue_id_ERICSSON-Create"}';  
 	
    // tempo di validita' nota per Wind
    var MS_article_validityTime_ToWind = '$Env{"MS_article_validityTime_ToWind"}'; 
	
 	
 	
 	//valori correnti del ticket
 	var MS_current_ticket_typeID = '$Env{"MS_current_ticket_typeID"}';
 	var MS_current_ticket_freetext15 = '$QData{"TicketFreeText15"}';
 	var MS_current_ticket_AmbitoTT = '$Env{"MS_current_ticket_WIND_AMBITOTT"}';
 	
 	var MS_current_ticket_TTL_Expired  = '$Env{"MS_current_ticket_TTL_Expired"}';
 	
 	var MS_current_ticket_Valid_Article_for_Wind = '$Env{"MS_current_ticket_Valid_Article_for_Wind"}';


 	
 	
	if ( queueDest == MS_queue_id_alarm_from_wind )
	{
         alert("Non puoi spostare ticket su questa coda.");
         return false;	
	}
	else if (queueDest == MS_queue_id_incident_for_wind ||
			queueDest == MS_queue_id_ericsson ||
			queueDest == MS_queue_id_ERICSSON_PM_WIND_OUT ||
			queueDest == MS_queue_id_MVNE_FE_Ericsson_Incident
	)
	{
		if ( MS_current_ticket_typeID == MS_ticketTypeID_IncidentToWind ||
			MS_current_ticket_typeID == MS_ticketTypeID_IncidentToPM ||
			MS_current_ticket_typeID == MS_ticketTypeID_IncidentToEricsson 
			
			|| document.getElementById("MyType").title == "SR-Alarm/Reclamo" 
    		|| document.getElementById("MyType").title == "SR-Escalation"
        	|| document.getElementById("MyType").title == "SR-Sollecito"
        	|| document.getElementById("MyType").title == "PR Master" 
			) //Incident per Wind, per PM o per Ericsson
		{

			if(MS_current_ticket_TTL_Expired == 1)
			{
				alert('$JSText{"TTL Expired: ticket non riapribile"}');
				return false;			
			}



			var category_ok = false;
			for (var i=0;i<MS_Category_Incident_PM_Wind_ARRAY.length;i++)
			{
				if( MS_current_ticket_freetext15 == MS_Category_Incident_PM_Wind_ARRAY[i])
				{
					category_ok = true;
					break;
				}
			}
			
			if(category_ok)
			{
				if( (queueDest == MS_queue_id_incident_for_wind || 
					queueDest == MS_queue_id_ERICSSON_PM_WIND_OUT) 
					&& MS_current_ticket_typeID == MS_ticketTypeID_IncidentToWind
				)
				{
					if(MS_current_ticket_Valid_Article_for_Wind > 0)
					{
						return MS_validate_for_Wind_common();
					}
					else
					{
						alert("Nota per Wind non presente o troppo vecchia (max " + MS_article_validityTime_ToWind + " sec)");
						return false;						
					}
				}
				else
				{
					return MS_validate_for_Wind_common();
				}

			}
			else
			{
				alert('$JSText{"Categoria non corretta per spostare il ticket sulla coda desiderata"}');
				return false;
			}
		} 
		else
		{
			alert('$JSText{"Tipo di ticket non compatibile con lo spostamento sulla coda desiderata"}');
			return false;	
		}

	}
	else if (queueDest == MS_queue_id_alarm_to_wind ||
			queueDest == MS_queue_id_MVNE_FE_Wind_Ericsson_Alarm ||
			queueDest == MS_queue_id_ERICSSON_WIND_OUT_Alarm ||
			queueDest == MS_queue_id_WIND_ERICSSON_OUT_Alarm ||
			queueDest == MS_queue_id_ERICSSON_OUT_Alarm ||
			queueDest == MS_queue_id_MVNE_FE_Ericsson_Alarm
	)  
    {
    	if ( MS_current_ticket_typeID == MS_ticketTypeID_AlarmToWind ||
			MS_current_ticket_typeID == MS_ticketTypeID_AlarmToEricsson ||
			MS_current_ticket_typeID == MS_ticketTypeID_AlarmToPM 
		) //ALARM per Wind, per PM o per Ericsson
    	{
    		var category_ok = false;
			for (var i=0;i<MS_Category_Alarm_PM_Wind_ARRAY.length;i++)
			{
				if(MS_current_ticket_freetext15 == MS_Category_Alarm_PM_Wind_ARRAY[i])
				{
					category_ok = true;
					break;
				}
			}
			
			if(category_ok)
			{
				return MS_validate_for_Wind_common();
			}
			else
			{
    			alert('$JSText{"Categoria con valore non possibile per questa tipologia di ticket"}');
    			return false;		
			}
    	}
    	else
    	{
			alert('$JSText{"Tipo di ticket non compatibile con lo spostamento sulla coda desiderata"}');
			return false;	
    	}
    } 
    
    
    return true;
}
